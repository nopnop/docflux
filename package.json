    "underscore": "^1.6.0",
    "underscore.string": "^2.3.3"
  },
  "devDependencies": {
    "concat-stream": "^1.4.6",
    "expect.js": "*",
    "jshint": "*",
    "mocha": "*"
  },
  "keywords": []
